<h2>Registrieren</h2>
 
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :multipart => true }) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :nachname %><br />
  <%= f.text_field :lastname %></div>

  <div><%= f.label :vorname %><br />
  <%= f.text_field :firstname %></div>

  <div><%= f.label :passwort %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :passwort_wiederholen %><br />
  <%= f.password_field :password_confirmation %></div>

 <div><%= f.label :Profilfoto_hochladen %><br />
  <%= f.file_field :photo %>
</div>


  <div><%= f.submit "Registrieren" %></div>
<% end %>
<%= render "devise/shared/links" %>
